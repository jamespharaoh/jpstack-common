<record
	name="simulator"
	type="major">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="slice"/>

		<code-field/>

		<!-- details -->

		<name-field/>

		<description-field/>

		<deleted-field/>

	</fields>

	<collections>

		<children-collection
			type="simulatorRoute"/>

	</collections>

	<priv-types>

		<priv-type
			name="Manage"
			description="Full control of an SMS simulator"
			template="yes"/>

	</priv-types>

	<senders>

		<sender
			name="Simulator"
			description="Simulator virtual sender"/>

	</senders>

</record>
