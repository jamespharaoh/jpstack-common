<console-module
	name="shn-product-image">

	<console-helper-provider
		object-name="shnProductImage"
		view-priv="product.subCategory.category.database:view"
		create-priv="subCategory.category.database:admin">

		<priv-key
			name="shnProductImage.manage"
			priv="product.subCategory.category.database:admin"/>

		<context-stuff
			name="shnProductImageName"
			field="index"/>

		<context-stuff
			name="shnProductId"
			field="product.id"/>

		<run-post-processor
			name="shnProduct"/>

	</console-helper-provider>

	<object-context
		name="shnProductImage"
		object-name="shnProductImage">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="shn-product-image"
			type="read-only">

			<columns>

				<index-field/>
				<description-field/>

				<image-field
					name="originalMedia"/>

				<yes-no-field
					name="active"/>

			</columns>

		</form>

		<form
			name="create"
			object-type="shn-product-image"
			type="create">

			<columns>

				<description-field/>

				<image-field
					name="originalMedia"/>

				<yes-no-field
					name="active"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="shn-product-image"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<index-field/>
				<description-field/>

				<image-field
					name="originalMedia"/>

				<yes-no-field
					name="active"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="shn-product-image"
			type="update">

			<columns>

				<description-field/>

				<image-field
					name="originalMedia"/>

				<yes-no-field
					name="active"/>
			</columns>

		</form>

	</forms>

</console-module>
