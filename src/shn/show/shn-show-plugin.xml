<plugin
	name="shn-show"
	package="shn.show">

	<plugin-dependencies>
		<plugin-dependency name="platform-scaffold"/>
		<plugin-dependency name="shn-core"/>
		<plugin-dependency name="shn-product"/>
		<plugin-dependency name="shn-supplier"/>
	</plugin-dependencies>

	<models>
		<model name="shnShow"/>
		<model name="shnShowGuest"/>
		<model name="shnShowItem"/>
		<model name="shnShowPresenter"/>
		<model name="shnShowType"/>
	</models>

	<fixtures>
		<fixture name="shnShow"/>
	</fixtures>

	<console-modules>
		<console-module name="shn-show"/>
		<console-module name="shn-show-guest"/>
		<console-module name="shn-show-item"/>
		<console-module name="shn-show-presenter"/>
		<console-module name="shn-show-type"/>
	</console-modules>

</plugin>
