<plugin
	name="shn-promo"
	package="shn.promo">

	<plugin-dependencies>
		<plugin-dependency name="platform-scaffold"/>
		<plugin-dependency name="shn-core"/>
		<plugin-dependency name="shn-product"/>
	</plugin-dependencies>

	<models>
		<model name="shnPromo"/>
		<model name="shnPromoItem"/>
		<model name="shnPromoType"/>
	</models>

	<fixtures>
		<fixture name="shnPromo"/>
	</fixtures>

	<console-modules>
		<console-module name="shn-promo"/>
		<console-module name="shn-promo-item"/>
		<console-module name="shn-promo-type"/>
	</console-modules>

</plugin>
