<console-module
	name="shn-supplier">

	<console-helper-provider
		object-name="shnSupplier">

		<priv-key
			name="shnSupplier.manage"
			priv="manage"/>

		<context-stuff
			name="shnSupplierName"
			field="name"/>

		<context-stuff
			name="shnDatabaseId"
			field="database.id"/>

		<run-post-processor
			name="shnDatabase"/>

	</console-helper-provider>

	<object-context
		name="shnSupplier"
		object-name="shnSupplier">

		<list>

			<object-list-page
				form="list"/>

			<object-create-page
				form="create"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-settings-page
				form="settings"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="shn-supplier"
			type="read-only">

			<columns>

				<parent-field/>
				<name-field/>
				<description-field/>

			</columns>

		</form>

		<form
			name="create"
			object-type="shn-supplier"
			type="create">

			<columns>

				<parent-field/>
				<name-field/>
				<description-field/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="shn-supplier"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<code-field/>
				<name-field/>
				<description-field/>
				<deleted-field/>

				<text-field
					name="publicName"/>

				<text-field
					name="contactName"/>

				<text-field
					name="contactAddress"/>

				<text-field
					name="contactPhone"/>

				<text-field
					name="contactNotes"/>

			</columns>

		</form>

		<form
			name="settings"
			object-type="shn-supplier"
			type="update">

			<columns>

				<parent-field/>
				<name-field/>
				<description-field/>
				<deleted-field/>

				<text-field
					name="publicName"
					nullable="yes"/>

				<text-field
					name="contactName"
					nullable="yes"/>

				<text-field
					name="contactAddress"
					nullable="yes"/>

				<text-field
					name="contactPhone"
					nullable="yes"/>

				<text-field
					name="contactNotes"
					nullable="yes"/>

			</columns>

		</form>

	</forms>

</console-module>
